<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nagar.manish.fitnessapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/Notepad"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            />
        <activity android:name=".Detailshow" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".BMICalculator" />
        <activity android:name=".Notepad"
            android:label="@string/Notepad"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".EditField"
            android:label="@string/change_task"></activity>
    </application>

</manifest>